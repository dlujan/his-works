{
  "expo": {
    "name": "HisWorks",
    "slug": "his-works",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon2-1024x1024.png",
    "scheme": "hisworks",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.daniellujan.hisworks",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FCE7CF",
        "foregroundImage": "./assets/images/android-icon2-512x512.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.daniellujan.hisworks"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon-96x96.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon2-1024x1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FCE7CF",
          "dark": {
            "backgroundColor": "#FCE7CF"
          }
        }
      ],
      "expo-mail-composer",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "02c37db9-69dc-4af9-82c6-0c0904cf63c7"
      }
    },
    "owner": "dlujan"
  }
}
